<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/note_pattern_item.wrapper"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <de.thkoeln.colab.fearlesschange.view.custom.NoteCard
        android:id="@+id/note_pattern_item.note_card"
        style="@style/NoteCard"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/default_margin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/note_pattern_item.pattern_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0">

    </de.thkoeln.colab.fearlesschange.view.custom.NoteCard>


    <de.thkoeln.colab.fearlesschange.view.custom.PatternCardPreview
        android:id="@+id/note_pattern_item.pattern_card"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/note_pattern_item.note_card"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0">

    </de.thkoeln.colab.fearlesschange.view.custom.PatternCardPreview>


</androidx.constraintlayout.widget.ConstraintLayout>